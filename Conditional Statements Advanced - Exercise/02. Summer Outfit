degrees = int(input())
day_time = input()

day_time_list = ["Morning", "Afternoon", "Evening"]
outfit = None
shoes = None

if 10 <= degrees <= 18:

    if day_time == day_time_list[0]:
        outfit = "Sweatshirt"
        shoes = "Sneakers"
    elif day_time == day_time_list[1]:
        outfit = "Shirt"
        shoes = "Moccasins"
    elif day_time == day_time_list[2]:
        outfit = "Shirt"
        shoes = "Moccasins"

elif 18 < degrees <= 24:

    if day_time == day_time_list[0]:
        outfit = "Shirt"
        shoes = "Moccasins"
    elif day_time == day_time_list[1]:
        outfit = "T-Shirt"
        shoes = "Sandals"
    elif day_time == day_time_list[2]:
        outfit = "Shirt"
        shoes = "Moccasins"

elif degrees >= 25:

    if day_time == day_time_list[0]:
        outfit = "T-Shirt"
        shoes = "Sandals"
    elif day_time == day_time_list[1]:
        outfit = "Swim Suit"
        shoes = "Barefoot"
    elif day_time == day_time_list[2]:
        outfit = "Shirt"
        shoes = "Moccasins"

print(f"It's {degrees} degrees, get your {outfit} and {shoes}.")
