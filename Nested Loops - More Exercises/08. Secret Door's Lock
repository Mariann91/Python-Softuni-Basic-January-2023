num1_limit = int(input())
num2_limit = int(input())
num3_limit = int(input())

for num1 in range(2, num1_limit + 1, 2):
    for num2 in range(1, num2_limit + 1):
        for num3 in range(2, num3_limit + 1, 2):
            
            if num2 == 2 or num2 == 3 or num2 == 5 or num2 == 7:
                print(f"{num1} {num2} {num3}")
