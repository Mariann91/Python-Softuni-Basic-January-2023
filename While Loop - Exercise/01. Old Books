wanted_book = input()

book_counter = 0

while True:
    book = input()
    
    if book == wanted_book:
        print(f"You checked {book_counter} books and found it.")
        break
    
    if book == "No More Books":
        print("The book you search is not here!")
        print(f"You checked {book_counter} books.")
        break
    
    book_counter += 1
